<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>BBDPDoctor</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>CorsFilter</filter-name>
    <filter-class>org.apache.catalina.filters.CorsFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CorsFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>LoginVerificationServlet.java</display-name>
    <servlet-name>LoginVerificationServlet.java</servlet-name>
    <servlet-class>bbdp.doctor.controller.LoginVerificationServlet.java</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginVerificationServlet.java</servlet-name>
    <url-pattern>/LoginVerificationServlet.java</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AccountSettingServlet.java</display-name>
    <servlet-name>AccountSettingServlet.java</servlet-name>
    <servlet-class>bbdp.doctor.controller.AccountSettingServlet.java</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AccountSettingServlet.java</servlet-name>
    <url-pattern>/AccountSettingServlet.java</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LogoutServlet.java</display-name>
    <servlet-name>LogoutServlet.java</servlet-name>
    <servlet-class>bbdp.doctor.controller.LogoutServlet.java</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet.java</servlet-name>
    <url-pattern>/LogoutServlet.java</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>bbdp.db.controller.ServletListener</listener-class>
  </listener>
  <context-param>
    <param-name>url</param-name>
    <param-value>jdbc:mysql://localhost/</param-value>
  </context-param>
  <context-param>
    <param-name>database</param-name>
    <param-value>bbdp</param-value>
  </context-param>
  <context-param>
    <param-name>user_name</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>password</param-name>
    <param-value>15192353</param-value>
  </context-param>
  <servlet>
    <servlet-name>PatientSearchServlet</servlet-name>
    <servlet-class>bbdp.doctor.controller.PatientSearchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PatientSearchServlet</servlet-name>
    <url-pattern>/PatientSearchServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>NotificationServlet</servlet-name>
    <servlet-class>bbdp.doctor.controller.NotificationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NotificationServlet</servlet-name>
    <url-pattern>/NotificationServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ClinicPushServlet</servlet-name>
    <servlet-class>bbdp.doctor.controller.ClinicPushServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClinicPushServlet</servlet-name>
    <url-pattern>/ClinicPushServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PatientBasicInformationServlet</servlet-name>
    <servlet-class>bbdp.doctor.controller.PatientBasicInformationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PatientBasicInformationServlet</servlet-name>
    <url-pattern>/PatientBasicInformationServlet</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>PatientFolderServlet</servlet-name>
    <servlet-class>bbdp.doctor.controller.PatientFolderServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PatientFolderServlet</servlet-name>
    <url-pattern>/PatientFolderServlet</url-pattern>
  </servlet-mapping>
</web-app>